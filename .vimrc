" required by vim-plug
    call plug#begin('~/.vim/plugged')

" Plugins
    Plug 'jiangmiao/auto-pairs'

" required by vim-plug
    call plug#end()
	set mouse=a

" editing
	set autoindent
	set smartindent

" interface
	set number
	set cursorline

" searching
	set hlsearch

" encoding
	set enc=utf-8
